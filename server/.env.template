# Environment Configuration
NODE_ENV="development" # Options: 'development', 'production'
PORT="8080"            # The port your server will listen on
HOST="localhost"       # Hostname for the server

# CORS Settings
CORS_ORIGIN="http://localhost:*" # Allowed CORS origin, adjust as necessary

# Rate Limiting
COMMON_RATE_LIMIT_WINDOW_MS="1000"  # Window size for rate limiting (ms)
COMMON_RATE_LIMIT_MAX_REQUESTS="20" # Max number of requests per window per IP

# Random string for signing JWTs
APP_JWT_SECRET=

# Your OpenAI key
OPEN_AI_KEY=

# Google Cloud credential file (often called service_account.json)
GOOGLE_APPLICATION_CREDENTIALS=./service_account.json
GOOGLE_OAUTH_CLIENT_ID=

# Twilio credentials
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
